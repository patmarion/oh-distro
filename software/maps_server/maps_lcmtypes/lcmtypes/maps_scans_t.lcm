package maps;

struct scans_t
{
  // timestamp of this message
  int64_t utime;

  // id of map
  int64_t map_id;

  // id of view
  int64_t view_id;

  // number of scans
  int32_t num_scans;

  // approximate payload size
  int32_t data_bytes;

  // transform from reference to cloud coords
  float transform[4][4];

  // scan data
  scan_t scans[num_scans];

  // sequence info
  int32_t chunk_id;
  int32_t num_chunks;
}

<?xml version="1.0"?>
<package format="2">
  <name>dual_arm_husky_ros_lcm_translators</name>
  <version>1.0.0</version>
  <description>Translates messages between the Clearpath Dual UR5 Husky (ROS) and the LCM ecosystem</description>

  <maintainer email="wolfgang.merkt@ed.ac.uk">Wolfgang Merkt</maintainer>

  <!-- Commonly used license strings: -->

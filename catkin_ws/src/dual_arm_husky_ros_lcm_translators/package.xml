  <url type="repository">https://github.com/openhumanoids/oh-distro-private</url>
  <url type="bugtracker">https://github.com/openhumanoids/oh-distro-private/issues</url>
  
  <author email="wolfgang.merkt@ed.ac.uk">Wolfgang Merkt</author>
  <author email="yiming.yang@ed.ac.uk">Yiming Yang</author>
  <author email="stoutheo@gmail.com">Theo Stouraitis</author>

  <buildtool_depend>catkin</buildtool_depend>
  <depend>nav_msgs</depend>
  <depend>move_base_msgs</depend>
  <depend>husky_msgs</depend>
  <depend>robotiq_force_torque_sensor_msgs</depend>
  <depend>sensor_msgs</depend>
